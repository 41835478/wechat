<!--pages/comment/comment.wxml-->
<view class="main">
<block wx:for="{{commentListData}}" wx:key="commentListData">
<view class="comment-box">
 <image class="userlogo"src='{{item.avatar}}'></image> 
 <view class="box">
 <view class="first">
<view class="name"> {{item.uname}}</view> <view class="time">{{item.addtime}}</view></view>
<view class="star">{{item.start_level}}</view> 
<view class="comment">{{item.comment}}</view>
</view>

</view>
</block>
</view>
