/* pages/collect/collect.wxss */
.page {
     margin: 0;
     padding-top: 20rpx;
}

.collect_ul {
     padding: 10rpx 30rpx 0 30rpx;
     box-sizing: border-box;
     display: flex;
     width: 100%;
     height: auto;
     background: #fff;
   
}
.collect_message {
     width: 345rpx;
     height: 150rpx;
     display: flex;
     flex-direction: column;
     justify-content: space-around;
     padding-left: 30rpx;
     padding-top: 30rpx;
     padding-bottom: 30rpx;
     box-sizing: border-box;
}
