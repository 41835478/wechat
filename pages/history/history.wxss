/* pages/history/history.wxss */
.page {
     width: 100%;
     height: 100vh;
     background: #fff;
     margin: 0;
     
}

.history_ul {
     padding: 10rpx 30rpx 0 30rpx;
     box-sizing: border-box;
     display: flex;
     width: 100%;
     height: auto;
     background: #fff;
   
}
.history_message {
     width: 345rpx;
     height: 150rpx;
     display: flex;
     flex-direction: column;
     justify-content: space-around;
     padding-left: 30rpx;
     padding-top: 30rpx;
     padding-bottom: 30rpx;
     box-sizing: border-box;
}
